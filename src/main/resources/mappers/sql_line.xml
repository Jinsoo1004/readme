<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.alt.readsearchservice.mapper.LineMapper">

    <resultMap id="line" type="com.alt.readsearchservice.domain.Line">
        <result property="idx" column="idx" />
        <result property="name" column="name" />
    </resultMap>

    <select id="getByName" resultMap="line">
        SELECT * FROM line

        WHERE name = #{name}
    </select>
</mapper>